{
    "presets": [
        [
            "next/babel",
            {
                "preset-env": {
                    "targets": {
                        "browsers": [">0.5%", "not ie 11", "not op_mini all"],
                        "node": true
                    }
                },
                "styled-jsx": {
                    "vendorPrefixes": false,
                    "plugins": [
                        [
                            "styled-jsx-plugin-stylus-nib",
                            {
                                "paths": ["./styles"],
                                "imports": ["stylus"]
                            }
                        ]
                    ]
                }
            }
        ],
        ["@babel/stage-0", { "decoratorsLegacy": true }]
    ],
    "plugins": [
        "lodash",
        "babel-plugin-root-import",
        [
            "ramda",
            {
                "useES": true
            }
        ]
    ]
}
