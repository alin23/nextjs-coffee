        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-ramda": "^1.6.2",
        "babel-plugin-reduxsauce-reducer": "^1.0.0",
        "babel-plugin-root-import": "^6.1.0",
        "coffee-loader": "^0.9.0",
        "coffeescript": "^2.3.1",
        "import-sort": "^5.2.0",
        "import-sort-parser-babylon": "^5.2.0",
        "import-sort-style-structured": "^1.0.0",
        "lodash-webpack-plugin": "^0.11.5",
        "micro-dev": "^3.0.0",
        "next-coffeescript": "1.3.0",
        "nib": "^1.1.2",
        "npm-run-all": "^4.1.3",
        "patch-package": "^5.1.1",
        "prettier-plugin-coffeescript": "github:alin23/prettier-plugin-coffeescript",
        "reactotron-apisauce": "^2.0.0",
        "reactotron-react-js": "^2.0.0",
        "reactotron-redux": "^2.0.0",
