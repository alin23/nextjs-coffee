{
    "scripts": {
        "watch:server": "coffee -c -M -w server",
        "watch:styles": "stylus -o static/css -I stylus -c -m -w stylus/bootstrap.styl stylus/app.styl",
        "compile:server": "coffee -c -M server",
        "compile:styles": "stylus -o static/css -I stylus -c -m stylus/bootstrap.styl stylus/app.styl",
        "start:dev": "micro-dev server.js --watch server --watch next.config.js --ignore '.*/.+\\.coffee'",
        "start:production": "env NODE_ENV=production micro server.js",
        "inspect": "node --inspect node_modules/.bin/micro-dev server.js --watch server --watch next.config.js --ignore '.*/.+\\.coffee'",
        "patch": "patch-package",
        "debug": "npm-run-all patch compile:** --parallel watch:** inspect",
        "dev": "npm-run-all patch compile:** --parallel watch:** start:dev",
        "build": "patch-package && env NODE_ENV=production next build",
        "start": "npm-run-all --sequential compile:server start:production"
    },
    "license": "UNLICENSED",
    "importSort": {
        ".coffee": {
            "parser": "coffee",
            "style": "eslint"
        },
        ".js, .jsx, .es6, .es": {
            "parser": "babylon",
            "style": "structured"
        },
        ".ts, .tsx": {
            "parser": "typescript",
            "style": "eslint"
        }
    },
    "devDependencies": {
        "@babel/core": "7.0.0-beta.42",
        "@babel/preset-stage-0": "7.0.0-beta.42",
        "@zeit/next-bundle-analyzer": "^0.1.2-canary.4",
